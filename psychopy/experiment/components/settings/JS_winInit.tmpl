function setupWin() {{
  // Start Code - component code to be run before the window creation
  // Setup the Window
  _.window = new Window({{
      size:{params[Window size (pixels)]}, //[2560, 1440],
      fullscr:{fullscr},
      screen:{params[Screen]},
      color: new Color({params[color]}),
      units: {params[Units]}
      }});
}}

