"commentdockeyword": "fore:#008000,back:#F0FFF0,bold,face:%(comment)s,size:%(size)d",
"commentdockeyworderror": "fore:#008000,back:#F0FFF0,bold,face:%(comment)s,size:%(size)d",
"string": "fore:#800080,face:%(code)s,size:%(size)d",
"character": "fore:#800080,face:%(code)s,size:%(size)d",
